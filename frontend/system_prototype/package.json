{
  "name": "campusflow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production next build",
    "build:deploy": "./scripts/build-production.sh",
    "start:production": "NODE_ENV=production next start",
    "preview": "next build && next start",
    "clean": "rm -rf .next out",
    "build:worker": "opennextjs-cloudflare build",
    "preview:worker": "opennextjs-cloudflare preview",
    "deploy:vercel": "vercel --prod",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=.next",
    "ingest:smoke": "tsx scripts/ingest/smoke-test.ts",
    "ingest:discover": "tsx scripts/ingest/01-discover.ts",
    "ingest:crawl": "tsx scripts/ingest/02-crawl.ts",
    "ingest:structure": "tsx scripts/ingest/03-structure.ts",
    "ingest:upsert": "tsx scripts/ingest/04-upsert.ts",
    "ingest:all": "npm run ingest:discover && npm run ingest:crawl && npm run ingest:structure && npm run ingest:upsert"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "next": "14.2.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@opennextjs/cloudflare": "^0.5.0",
    "@playwright/test": "^1.57.0",
    "@types/node": "^20.17.10",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.18",
    "playwright": "^1.57.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "wrangler": "^3.95.0"
  }
}
